<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomToolbar">
        <attr name="title" format="string"/>
        <attr name="subTitle" format="string"/>
    </declare-styleable>

    <declare-styleable name="BaseProjectButton">
        <attr name="android:title" />
        <attr name="android:icon" />
        <attr name="android:textColor"/>
        <attr name="backgroundColor"/>
    </declare-styleable>

    <declare-styleable name="ItemSettingDisplay">
        <attr name="display_title" format="string" />
        <attr name="display_setting_text" format="string" />
        <attr name="display_setting_icon" format="reference" />
        <attr name="display_divider" format="boolean" />
    </declare-styleable>

</resources>
